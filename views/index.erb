<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <title>LiveRover</title>
  <link rel="stylesheet" type="text/css" href="css/bootstrap.css" />
  <link rel="stylesheet" type="text/css" href="css/style.css" />
  <link href='http://fonts.googleapis.com/css?family=Lobster|Open+Sans:400,300,700' rel='stylesheet' type='text/css'>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
  <script src="http://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.3.1/underscore-min.js"></script>
  <script src="http://cdnjs.cloudflare.com/ajax/libs/backbone.js/0.9.0/backbone-min.js"></script>
  <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?key=AIzaSyCJvqOiQah_2LJ7bMnpvgAhYPf5CDGMQ-0&sensor=true"></script>
  <script type="text/javascript" src="js/map.js"></script>
  <script type="text/javascript" src="js/date.js"></script>
  <script type="text/javascript" src="js/app.js"></script>
  <!--[if IE]>
    <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
</head>

<body>
  <div id="wrapper">
    <div id="header">
      <div id="logo">
        LiveRover
      </div>
      <div id="date">
        <a id="prev" class="disabled" href="#"><span class="icon">←</span></a><div id="date-value"></div><a id="next" href="#"><span class="icon">→</span></a>
      </div>
      <div id="credit">
        powered by the <b>SeatGeek API</b>
      </div>
    </div>
    <div id="map"></div>
  </div>
  
  <script type="text/html" id="info-window">
    <h4 class="event-title"><a target="_blank" href="{{ link }}">{{ title }}</a></h4>
    <div class="venue">{{ venue }}</div>
    <div class="time">{{ time }}</div>
  </script>

  
</body>
</html>